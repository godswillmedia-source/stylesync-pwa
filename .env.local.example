# StyleSync PWA Environment Variables
# Copy this file to .env.local and fill in your values

# Supabase Agent URL (Edge Function)
NEXT_PUBLIC_AGENT_URL=https://YOUR-PROJECT-ID.supabase.co/functions/v1/stylesync-agent

# Stripe (for payments)
NEXT_PUBLIC_STRIPE_PUBLISHABLE_KEY=pk_test_...

# VAPI Voice AI (new - for voice assistant)
NEXT_PUBLIC_VAPI_PUBLIC_KEY=your_vapi_public_key_here
NEXT_PUBLIC_VAPI_ASSISTANT_ID=your_vapi_assistant_id_here
